<% if(error && error.length) { %>
<div class="ui negative message">
    <i class="close icon"></i>
    <div class="header">
        <p><%= error %> !</p>
    </div>
</div>
<% } %>
<% if(success && success.length > 0) { %>
<div class="ui positive message">
    <i class="close icon"></i>
    <div class="center aligned">
        <strong><%= success %></strong>
    </div>
</div>
<% } %>